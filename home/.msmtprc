# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.msmtp.log

# Account at Google with oauth2 access                                    
account ttlong221@clc.fitus.edu.vn
from ttlong221@clc.fitus.edu.vn
user ttlong221@clc.fitus.edu.vn
auth oauthbearer
passwordeval oama access ttlong221@clc.fitus.edu.vn
host smtp.gmail.com
port 587
tls on
tls_trust_file /etc/ssl/certs/ca-certificates.crt

# Set a default account
account default: ttlong221@clc.fitus.edu.vn

